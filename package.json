{
  "name": "aspiration-ai-internship",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:JamesMcGuigan/aspiration-ai-internship.git",
  "author": "JamesMcGuigan <james.mcguigan.github@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "yarn clean:python; yarn clean:typescript",
    "clean:python": "find ./src/ -name '__pycache__' -or -name '*.py[cod]' -delete",
    "clean:typescript": "find ./.ts/ ./src/*/typescript/ -name '*.js' -or -name '*.map' -delete",
    "tsc:build": "yarn clean:typescript; tsc",
    "tsc:watch": "yarn clean:typescript; tsc -w",
    "run": "yarn run:module1",
    "run:module1": "yarn run:module1:python3; yarn run:module1:typescript;",
    "run:module1:python3": "source venv/bin/activate; python3 src/module_1/python3/main.py",
    "run:module1:typescript": "yarn tsc; node --experimental-modules --es-module-specifier-resolution=node src/module_1/typescript/index.js"
  },
  "dependencies": {
    "csv": "^5.1.1",
    "datejs": "^1.0.0-rc3",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "typescript": "^3.5.2",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.10",
    "ts-node": "^8.3.0"
  }
}
